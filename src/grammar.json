{
  "name": "ungrammar",
  "rules": {
    "grammar": {
      "type": "REPEAT1",
      "content": {
        "type": "SYMBOL",
        "name": "node"
      }
    },
    "node": {
      "type": "SEQ",
      "members": [
        {
          "type": "FIELD",
          "name": "name",
          "content": {
            "type": "SYMBOL",
            "name": "ident"
          }
        },
        {
          "type": "STRING",
          "value": "="
        },
        {
          "type": "FIELD",
          "name": "rule",
          "content": {
            "type": "SYMBOL",
            "name": "_rule"
          }
        }
      ]
    },
    "_atom_rule": {
      "type": "SEQ",
      "members": [
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SYMBOL",
              "name": "ident"
            },
            {
              "type": "SYMBOL",
              "name": "token_ident"
            },
            {
              "type": "SEQ",
              "members": [
                {
                  "type": "STRING",
                  "value": "("
                },
                {
                  "type": "SYMBOL",
                  "name": "_rule"
                },
                {
                  "type": "STRING",
                  "value": ")"
                }
              ]
            },
            {
              "type": "SYMBOL",
              "name": "labeled"
            }
          ]
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SYMBOL",
              "name": "modifier"
            },
            {
              "type": "BLANK"
            }
          ]
        }
      ]
    },
    "labeled": {
      "type": "SEQ",
      "members": [
        {
          "type": "FIELD",
          "name": "label",
          "content": {
            "type": "SYMBOL",
            "name": "ident"
          }
        },
        {
          "type": "STRING",
          "value": ":"
        },
        {
          "type": "SYMBOL",
          "name": "_atom_rule"
        }
      ]
    },
    "modifier": {
      "type": "CHOICE",
      "members": [
        {
          "type": "STRING",
          "value": "?"
        },
        {
          "type": "STRING",
          "value": "*"
        }
      ]
    },
    "_rule": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "seq_rule"
        },
        {
          "type": "SYMBOL",
          "name": "alt_rule"
        }
      ]
    },
    "alt_rule": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "seq_rule"
        },
        {
          "type": "REPEAT1",
          "content": {
            "type": "SEQ",
            "members": [
              {
                "type": "STRING",
                "value": "|"
              },
              {
                "type": "SYMBOL",
                "name": "seq_rule"
              }
            ]
          }
        }
      ]
    },
    "seq_rule": {
      "type": "REPEAT1",
      "content": {
        "type": "SYMBOL",
        "name": "_atom_rule"
      }
    },
    "ident": {
      "type": "PATTERN",
      "value": "[a-zA-Z_]+"
    },
    "token_ident": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "'"
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "PATTERN",
            "value": "[^\\']"
          }
        },
        {
          "type": "IMMEDIATE_TOKEN",
          "content": {
            "type": "STRING",
            "value": "'"
          }
        }
      ]
    },
    "comment": {
      "type": "TOKEN",
      "content": {
        "type": "SEQ",
        "members": [
          {
            "type": "STRING",
            "value": "//"
          },
          {
            "type": "PATTERN",
            "value": ".*"
          }
        ]
      }
    }
  },
  "extras": [
    {
      "type": "PATTERN",
      "value": "\\s"
    },
    {
      "type": "SYMBOL",
      "name": "comment"
    }
  ],
  "conflicts": [
    [
      "_rule"
    ],
    [
      "_atom_rule"
    ],
    [
      "alt_rule"
    ],
    [
      "seq_rule"
    ]
  ],
  "precedences": [],
  "externals": [],
  "inline": [],
  "supertypes": []
}

