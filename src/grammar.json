{
  "name": "ungrammar",
  "rules": {
    "grammar": {
      "type": "REPEAT",
      "content": {
        "type": "SYMBOL",
        "name": "node"
      }
    },
    "node": {
      "type": "PREC",
      "value": 4,
      "content": {
        "type": "SEQ",
        "members": [
          {
            "type": "FIELD",
            "name": "name",
            "content": {
              "type": "SYMBOL",
              "name": "ident"
            }
          },
          {
            "type": "STRING",
            "value": "="
          },
          {
            "type": "FIELD",
            "name": "rule",
            "content": {
              "type": "SYMBOL",
              "name": "_compound_rule"
            }
          }
        ]
      }
    },
    "_compound_rule": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "or_rule"
        },
        {
          "type": "SYMBOL",
          "name": "and_rule"
        },
        {
          "type": "SYMBOL",
          "name": "_single_rule"
        },
        {
          "type": "SYMBOL",
          "name": "parenthesized"
        }
      ]
    },
    "parenthesized": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "("
        },
        {
          "type": "SYMBOL",
          "name": "_compound_rule"
        },
        {
          "type": "STRING",
          "value": ")"
        }
      ]
    },
    "_single_rule": {
      "type": "SYMBOL",
      "name": "rule"
    },
    "or_rule": {
      "type": "PREC_LEFT",
      "value": 3,
      "content": {
        "type": "SEQ",
        "members": [
          {
            "type": "SYMBOL",
            "name": "_compound_rule"
          },
          {
            "type": "STRING",
            "value": "|"
          },
          {
            "type": "SYMBOL",
            "name": "_compound_rule"
          }
        ]
      }
    },
    "and_rule": {
      "type": "PREC_LEFT",
      "value": 2,
      "content": {
        "type": "SEQ",
        "members": [
          {
            "type": "SYMBOL",
            "name": "_compound_rule"
          },
          {
            "type": "SYMBOL",
            "name": "_compound_rule"
          }
        ]
      }
    },
    "rule": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "ident"
        },
        {
          "type": "SYMBOL",
          "name": "token_ident"
        },
        {
          "type": "SEQ",
          "members": [
            {
              "type": "SYMBOL",
              "name": "rule"
            },
            {
              "type": "STRING",
              "value": "?"
            }
          ]
        },
        {
          "type": "SEQ",
          "members": [
            {
              "type": "SYMBOL",
              "name": "rule"
            },
            {
              "type": "STRING",
              "value": "*"
            }
          ]
        },
        {
          "type": "SEQ",
          "members": [
            {
              "type": "FIELD",
              "name": "label",
              "content": {
                "type": "SYMBOL",
                "name": "ident"
              }
            },
            {
              "type": "STRING",
              "value": ":"
            }
          ]
        }
      ]
    },
    "ident": {
      "type": "PATTERN",
      "value": "[a-zA-Z]+"
    },
    "token_ident": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "'"
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "PATTERN",
            "value": "[^']"
          }
        },
        {
          "type": "IMMEDIATE_TOKEN",
          "content": {
            "type": "STRING",
            "value": "'"
          }
        }
      ]
    },
    "comment": {
      "type": "TOKEN",
      "content": {
        "type": "SEQ",
        "members": [
          {
            "type": "STRING",
            "value": "//"
          },
          {
            "type": "PATTERN",
            "value": ".*"
          }
        ]
      }
    }
  },
  "extras": [
    {
      "type": "PATTERN",
      "value": "\\s"
    },
    {
      "type": "SYMBOL",
      "name": "comment"
    }
  ],
  "conflicts": [],
  "precedences": [],
  "externals": [],
  "inline": [],
  "supertypes": []
}

